        <header>
            <%= link_to logo, root_path %>
            <nav class="round">
                <ul>
                    <li><%= link_to "Home", root_path %></li>
                    <li><%= link_to "Help", help_path %></li>
                    <% if signed_in? %>
                    <li><%= link_to "Logout", signout_path, :method => :delete %></li>
                    <li><%= link_to "Profile", current_user %></li>
                    <% else %>
                    <li><%= link_to "Login", signin_path %></li>
                    <% end %>
                </ul>
            </nav>
        </header>
